{
  "compilerOptions": {
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": ".",
    "paths": {
      "@tests/*": [
        "./*"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@support/*": [
        "support/*"
      ],
      "@cypress/*": [
        "../cypress/*"
      ],
      "@root/*": [
        "../*"
      ]
    }
  }
}
