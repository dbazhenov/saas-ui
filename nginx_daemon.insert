log_format  json  escape=json '{"time": "$msec","time_iso8601": "$time_iso8601","remote_addr": "$remote_addr","remote_user": "$remote_user","request": "$request","status": "$status","request_uri": "$request_uri","request_method": "$request_method","request_time": "$request_time","request_length": "$request_length","body_bytes_sent": "$body_bytes_sent","http_host": "$http_host","http_referer": "$http_referer","http_user_agent": "$http_user_agent","http_x_forwarded_for": "$http_x_forwarded_for","http_x_b3_traceid": "$http_x_b3_traceid","upstream_addr": "$upstream_addr","source": "nginx"}';
